<html>
	<body>
		<form name="frmSignIn" method="POST" action="j_security_check">
			<table>
				<tr>
					<td>Username</td>
					<td>
						<input maxlength="20" size="12" name="j_username" type="text"/>
					</td>
				</tr>
				<tr>
					<td>Password</td>
					<td>
						<input maxlength="20" size="12" name="j_password" type="password"/>
					</td>
				</tr>
			</table>
			<input type="submit" value="Login"/>
		</form>
	</body>
</html>
